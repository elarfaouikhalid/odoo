      <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
      <script>
        $('.formsubmit').submit(function(e) {
        e.preventDefault();
       
        $.ajax({
        method:"POST",
        url: "/patient/create",
        data:$(this).serialize(),
        success: function(data){
               Swal.fire({
                icon: 'success',
                title: 'Patient added successfully',
                confirmButtonText: 'Cool',
              });
        }});  
      });
      </script>
